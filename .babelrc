{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "Chrome >= 59",
            "FireFox >= 44",
            "Safari >= 7",
            "Explorer 11",
            "last 4 Edge versions"
          ]
        }
      }
    ],
    [ "@babel/preset-typescript" ],
    [ "@babel/preset-react" ]
  ],

  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      { "legacy": true }
    ],
    [
      "@babel/plugin-proposal-class-properties",
      { "loose": true }
    ],
    [ "@babel/proposal-object-rest-spread" ],
    [ "@babel/transform-runtime" ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}